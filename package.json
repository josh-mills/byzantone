{
  "name": "byz",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf public/dist && tsc && npx tailwindcss -o public/dist/main.css --minify && npx elm-watch make --optimize",
    "codegen": "node codegen/run.js",
    "dev": "concurrently \"npm run tailwind\" \"npm run start\" \"npx elm-watch hot\" \"npm run watch\"",
    "start": "node server.js",
    "tailwind": "npx tailwindcss -o public/dist/main.css --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "nodemon --watch 'src/chant_engine.ts' --exec 'tsc'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "elm-codegen": "^0.5.3",
    "elm-live": "^4.0.2",
    "elm-test": "^0.19.1-revision12",
    "elm-watch": "^1.1.2",
    "nodemon": "^2.0.22",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
}
