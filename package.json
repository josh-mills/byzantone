{
  "name": "byz",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "rm -rf public && tsc && npx @tailwindcss/cli -i src/main.css -o public/dist/main.css --minify && npx elm-watch make --optimize && node scripts/applyHash.mjs",
    "clean": "rm -rf public elm-stuff && mkdir public && cp src/index.html public/",
    "codegen": "node codegen/run.js",
    "dev": "npm run clean && concurrently \"npm run tailwind\" \"npm run start\" \"npx elm-watch hot\" \"npm run watch\"",
    "review": "elm-format review/src/ --yes && elm-review",
    "review-fix": "elm-format review/src/ --yes && elm-review --fix",
    "start": "node server.cjs",
    "tailwind": "npx @tailwindcss/cli -i src/main.css -o public/dist/main.css --watch",
    "test": "npx elm-test",
    "watch": "nodemon --watch src --ext ts --exec 'npx tsc'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "elm-codegen": "^0.5.3",
    "elm-format": "^0.8.8",
    "elm-live": "^4.0.2",
    "elm-review": "^2.13.5",
    "elm-test": "^0.19.1-revision12",
    "elm-watch": "^1.1.2",
    "nodemon": "^2.0.22",
    "tailwindcss": "^4.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
}
